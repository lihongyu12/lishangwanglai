/* pages/C/C.wxss */
.C{
 background-color:rgba(233, 233, 233, 0.1);
  height: 504px;
  position: relative;
  padding-top:10px; 
  box-sizing: border-box
}
.list{
  font-size:20px;
  font-weight: 600;
  line-height: 3em;
  margin:7px 6px; 
  border-radius: 5px;
  padding:0px 7px; 
  background-color: #fff;
}
.list:nth-child(1){
  margin-top:0px; 
}
.list image{
  width: 30px;
  height: 30px;
  margin-right: 10px;
  background-color: #ddd;
  vertical-align: middle;
}
.r{
  float: right;
  color: #ddd;
}
.fnex,.liuy{
position: absolute;
font-size: 12px;
padding:4px 15px;
text-align: center;
right: 10px; 
border-radius: 10px;
background-color: #fff;
box-shadow: 0px 1px 5px #ddd;
}
.fnex{
  bottom: 70px;
}
.liuy{
  bottom: 40px;
}