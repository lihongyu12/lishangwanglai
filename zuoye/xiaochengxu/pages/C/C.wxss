/* pages/C/C.wxss */
.C{
 background-color:rgba(233, 233, 233, 0.1);
 height: 1050rpx;
  position: relative;
  padding-top:20rpx; 
  box-sizing: border-box
}
.list{
  font-size:40rpx;
  font-weight: 600;
  line-height: 3em;
  margin:28rpx 12rpx; 
  border-radius: 5px;
  padding:0px 14rpx; 
  background-color: #fff;
}
.list:nth-child(1){
  margin-top:0px; 
}
.list image{
  width: 80rpx;
  height: 80rpx;
  margin-right: 20rpx;
  vertical-align: middle;
}
.r{
  float: right;
  color: #ddd;
}
.fnex,.liuy{
position: absolute;
font-size: 24rpx;
padding:8rpx 30rpx;
text-align: center;
right: 20rpx; 
border-radius: 10px;
background-color: #fff;
box-shadow: 0px 1px 5px #ddd;
}
.fnex{
  bottom: 140rpx;
}
.liuy{
  bottom: 80rpx;
}